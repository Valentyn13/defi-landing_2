{
    "version": 2,
    "builds": [
        {
            "src": "main.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest":"main.js"
    },
        {
            "src":"/api/swap/transaction",
            "dest":"./routes/transaction.route"
        },
        {
            "src":"/api/swap/getTransactions",
            "dest":"./routes/gerTransaction.route"
        }
    ]
}